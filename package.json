{
  "name": "flask-react-notes",
  "version": "1.0.0",
  "description": "Node package manager for Flask-React web server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-react": "babel node_modules/react --out-dir server/static/node_modules/react --copy-files",
    "copy-react-dom": "babel node_modules/react-dom --out-dir server/static/node_modules/react-dom --copy-files",
    "watch-jsx":"npx babel --watch src --out-dir server/static/js/components --presets=@babel/preset-react",
    "dev":"npm run watch-jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppak10/Flask-React-Notes.git"
  },
  "author": "ppak10",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ppak10/Flask-React-Notes/issues"
  },
  "homepage": "https://github.com/ppak10/Flask-React-Notes#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
